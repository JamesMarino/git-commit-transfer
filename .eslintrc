{
    "rules": {
        "no-await-in-loop": 1,
        "no-confusing-arrow": 0,
        "no-nested-ternary": 0,
        "camelcase": 0,
        "no-mixed-operators": 0,
        "arrow-parens": 0,
        "no-underscore-dangle": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "no-return-assign": 1
    }
}
